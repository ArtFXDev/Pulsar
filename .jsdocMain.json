{
  "opts": {
    "encoding": "utf8",
    "readme": "./main/README.md",
    "destination": "./documentation/backend",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "title": "Pulsar Backend",
      "menu": [
        {
          "title": "pulsarvfx.com",
          "link": "https://pulsarvfx.com/",
          "target": "_blank"
        },
        {
          "title": "Github",
          "link": "https://github.com/lgeertsen/Pulsar",
          "target": "_blank"
        },
        {
          "title": "Main Doc",
          "link": "https://doc.pulsarvfx.com"
        },
        {
          "title": "Frontend Doc",
          "link": "https://doc.pulsarvfx.com/frontend"
        }
      ]
    }
  },
  "plugins": [
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
